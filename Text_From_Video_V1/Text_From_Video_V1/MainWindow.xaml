                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.062"/>
                    <TranslateTransform/>
                </TransformGroup>
            </CheckBox.RenderTransform>
        </CheckBox>
        <CheckBox x:Name="bcg" Content="Adjustbcg" HorizontalAlignment="Left" Margin="436,324,0,0" VerticalAlignment="Top" Width="76" Height="27" RenderTransformOrigin="0.5,0.5">
            <CheckBox.RenderTransform>
